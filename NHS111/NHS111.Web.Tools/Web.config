<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="LogFileAppenderError" type="log4net.Appender.FileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <param name="File" value="Log.Error.NHS111.Web.Tools.txt" />
      <param name="AppendToFile" value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="250MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <appender name="LogFileAppenderDebug" type="log4net.Appender.FileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <param name="File" value="Log.Debug.NHS111.Web.Tools.txt" />
      <param name="AppendToFile" value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="250MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppenderDebug" />
      <appender-ref ref="LogFileAppenderInfo" />
      <appender-ref ref="LogFileAppenderError" />
    </root>

  </log4net>
  <appSettings file="C:\Configurations\nhs111-shared-resources\shared.debug.config">
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="RedisConnectionString" value="madeup.redis.net:6380,abortConnect=false,ssl=true,password=madeup=" />
    <add key="GPSearchApiUrl" value="http://gpsearch.net/surgery/?name={0}" />
    <add key="BusinessDosCheckCapacitySummaryUrl" value="http://localhost:9004/DOSapi/CheckCapacitySummary" />
    <add key="DOSMobileBaseUrl" value="http://localhost/" />
    <add key="dos_mobile_credential_user" value="test" />
    <add key="dos_mobile_credential_password" value="test" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="BusinessApiListOutcomesUrl" value="http://localhost:9001/outcome/list/" />
  </appSettings>
  <system.web>
